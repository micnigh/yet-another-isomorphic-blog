{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            0,
        ],
        "no-console": [
            0,
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jasmine": true,
        "phantomjs": true,
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "globals": {

    }
}
